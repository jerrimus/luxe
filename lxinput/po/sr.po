msgid "High"
msgstr "Високо"

#: ../data/lxinput.ui.h:4
msgid "Low"
msgstr "Ниско"

#: ../data/lxinput.ui.h:5
msgid "Fast"
msgstr "Брзо"

#: ../data/lxinput.ui.h:6
msgid "Slow"
